<% @title = "Musicians | Discover new music" %>
<div id="musician-list" class="row">
  <div class="col-md-6 col-md-offset-3">
    <div class="page-header text-center">
      <h2>Musicians</h2>
      <p class="text-faded">Check out our musicians. Click the username and learn more about them!</p>
    </div>
    <div class="text-right" style="color: #7C8792;">
      <small><i class="fa fa-music" aria-hidden="true"></i> concerts </small> |
      <small><i class="fa fa-bookmark" aria-hidden="true"></i> bookmarks </small>
    </div>
    <%= render 'musicians', { profiles: visible_profiles } %>
  </div>
</div>
