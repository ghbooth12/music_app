<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <div class="page-header">
      <h3 class="text-center">Add New Concert</h3>
    </div>

    <%= render 'form' %>

    <div class="pull-left">
      <% if user_profile %>
        <%= link_to 'Back', edit_user_profile_path(current_user, user_profile), class: 'btn btn-default' %>
      <% else %>
        <%= link_to 'Back', new_user_profile_path(current_user), class: 'btn btn-default' %>
      <% end %>
    </div>
  </div>
</div>
