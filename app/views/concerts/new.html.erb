<div class="row">
  <div class="col-md-10 col-md-offset-1">
    <div class="page-header">
      <h3 class="text-center">Add New Concert</h3>
    </div>

    <%= render 'form' %>

    <div class="pull-left">
      <% if @user.profiles.first %>
        <%= link_to 'Back', edit_user_profile_path(@user, @user.profiles.first), class: 'btn btn-default' %>
      <% else %>
        <%= link_to 'Back', new_user_profile_path(@user), class: 'btn btn-default' %>
      <% end %>
    </div>
  </div>
</div>
